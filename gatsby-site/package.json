{
  "name": "panvala-gatsby-site",
  "private": true,
  "version": "0.1.0",
  "dependencies": {
    "ethers": "^4.0.37",
    "gatsby": "^2.15.20",
    "gatsby-image": "^2.2.20",
    "gatsby-plugin-manifest": "^2.2.18",
    "gatsby-plugin-netlify": "^2.1.18",
    "gatsby-plugin-offline": "^3.0.8",
    "gatsby-plugin-react-helmet": "^3.1.8",
    "gatsby-plugin-sharp": "^2.2.25",
    "gatsby-source-filesystem": "^2.1.26",
    "gatsby-transformer-sharp": "^2.2.16",
    "netlify-cli": "^2.17.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.17",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.10.2"
  },
  "scripts": {
    "build": "gatsby build",
    "deploy:staging": "npx netlify deploy --site $NETLIFY_SITE_ID --auth $NETLIFY_AUTH_TOKEN --dir=public",
    "deploy:prod": "npx netlify deploy --site $NETLIFY_SITE_ID --auth $NETLIFY_AUTH_TOKEN --prod --dir=public",
    "invalidate:staging": "aws cloudfront create-invalidation --distribution-id $GATSBY_STAGING_DIST_ID --paths '/*'",
    "invalidate:production": "aws cloudfront create-invalidation --distribution-id $GATSBY_PRODUCTION_DIST_ID --paths '/*'",
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "gatsby serve",
    "serve": "gatsby serve",
    "test": "jest",
    "test:verbose": "jest --verbose"
  }
}
